<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>Mnubo.SmartObjects.Client</id>
        <version>3.0.0</version>
        <authors>David Francoeur,David Masse,Goruti,Jeremy Attali,Julien Letrouit,Manuel Vonthron,Mauro Arias,Jonathan Hallee</authors>
        <owners>mnubo, inc.</owners>
        <licenseUrl>https://opensource.org/licenses/apache-2.0</licenseUrl>
        <projectUrl>https://github.com/mnubo/smartobjects-net-client</projectUrl>
        <iconUrl>https://raw.githubusercontent.com/mnubo/smartobjects-net-client/master/mnubo.png</iconUrl>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <description>mnubo SmartObjects .Net SDK client</description>
        <releaseNotes>
            * Migrate to .NET 5.0
	    </releaseNotes>
        <copyright>Copyright 2021</copyright>
        <dependencies>
            <group targetFramework="net5.0">
                <dependency id="Newtonsoft.Json" version="12.0.3" />
                <dependency id="Polly" version="7.2.1" />

                <dependency id="System.Collections.Immutable" version="5.0.0" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="smartobjects-net-client\bin\Release\*.dll" target="lib/net5.0" />
        <file src="smartobjects-net-client\bin\Release\*.exe" exclude="**vshost**" target="lib/net5.0" />
        <file src="smartobjects-net-client\bin\Release\*.config" target="lib/net5.0" />
        <file src="smartobjects-net-client\bin\Release\*.xml" target="lib/net5.0" />
        <file src="smartobjects-net-client\bin\Release\*.pdb" target="lib/net5.0" />

        <file src="smartobjects-net-client\**\*.cs" exclude="obj\**" target="src" />
    </files>
</package>
